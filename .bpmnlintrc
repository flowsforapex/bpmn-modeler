{
  "extends": [
    "bpmnlint:recommended"
  ],
  "rules": {
    "conditional-flows": "off",
    "no-inclusive-gateway": "off",
    "no-gateway-join-fork": "off",
    "label-required": "warn",
    "apex/max-id-length": "error",
    "apex/merge-split-gateways": "warn",
    "apex/start-events": "warn",
    "apex/conditional-sequence-flows": "warn",
    "apex/iterations-loop": "warn"
  }
}