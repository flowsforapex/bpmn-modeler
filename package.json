{
  "name": "bpmn-modeler",
  "version": "1.0.0",
  "description": "BPMN Modeler with customised Properties Panel",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev-demo",
    "browser": "npm start -- -- --open --hot",
    "build-all": "npm-run-all build-prod build-dev build-demo",
    "build-demo": "webpack --mode production --config webpack.config.demo.js",
    "build-dev": "webpack --mode development --config webpack.config.dev.js",
    "build-prod": "webpack --mode production --config webpack.config.prod.js",
    "dev-demo": "webpack-dev-server --mode development --config webpack.config.demo.js",
    "dev-demo-prod": "webpack-dev-server --mode production --config webpack.config.demo.js",
    "lint": "eslint ./",
    "update-dependencies": "npx remove-node-modules && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mt-ag/bpmn-modeler.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mt-ag/bpmn-modeler/issues"
  },
  "homepage": "https://github.com/mt-ag/bpmn-modeler#readme",
  "dependencies": {
    "bpmn-js": "^8.7.2",
    "bpmn-js-bpmnlint": "^0.18.0",
    "bpmn-js-properties-panel": "^0.44.0",
    "bpmnlint": "^7.2.1",
    "bpmnlint-loader": "^0.1.5",
    "bpmnlint-plugin-apex": "file:bpmnlint-plugin-apex",
    "camunda-bpmn-moddle": "^5.1.2",
    "diagram-js": "^7.4.0",
    "remove-node-modules": "^1.7.6"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^6.3.1",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-webpack": "^1.2.5",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "webpack": "^4",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
