{
  "name": "bpmn-modeler",
  "version": "1.0.0",
  "description": "BPMN Modeler with customised Properties Panel",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev-demo",
    "browser": "npm start -- -- --open --hot",
    "build-all": "npm-run-all build-prod build-dev build-demo",
    "build-demo": "webpack --config webpack.config.demo.js",
    "build-dev": "webpack --config webpack.config.dev.js",
    "build-prod": "webpack --config webpack.config.prod.js",
    "dev-demo": "webpack-dev-server --mode development --config webpack.config.demo.js",
    "dev-demo-prod": "webpack-dev-server --mode production --config webpack.config.demo.js",
    "lint": "eslint ./",
    "update-dependencies": "npx remove-node-modules && npm i",
    "get-translations": "node modules/translationModule/translation-extractor"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mt-ag/bpmn-modeler.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mt-ag/bpmn-modeler/issues"
  },
  "homepage": "https://github.com/mt-ag/bpmn-modeler#readme",
  "dependencies": {
    "@bpmn-io/add-exporter": "^0.2.0",
    "@bpmn-io/properties-panel": "^3.23.0",
    "bpmn-js": "^17.9.2",
    "bpmn-js-bpmnlint": "^0.22.3",
    "bpmn-js-color-picker": "^0.7.1",
    "bpmn-js-properties-panel": "^5.23.0",
    "bpmnlint": "^10.3.0",
    "bpmnlint-loader": "^0.1.6",
    "bpmnlint-plugin-apex": "file:bpmnlint-plugin-apex",
    "camunda-bpmn-moddle": "^7.0.1",
    "css-loader": "^7.1.2",
    "diagram-js": "^14.10.0",
    "file-loader": "^6.2.0",
    "inherits": "^2.0.4",
    "lodash": "^4.17.21",
    "min-dash": "^4.2.1",
    "min-dom": "^5.1.1",
    "monaco-editor": "^0.49.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "remove-node-modules": "^1.7.6",
    "style-loader": "^4.0.0"
  },
  "devDependencies": {
    "acorn": "^8.12.1",
    "camunda-bpmn-js-behaviors": "^1.6.1",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-webpack": "^1.2.5",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gettext-extractor": "^3.8.0",
    "less": "^4.2.0",
